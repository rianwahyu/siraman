<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:background="#F1F1F1">
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.75"
                android:id="@+id/frameNumPad"
                android:visibility="gone">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:srcCompat="@drawable/ic_cancel"
                        android:layout_gravity="end"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:id="@+id/imgCloseNumPad"
                        tools:ignore="MissingConstraints"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintRight_toRightOf="parent"/>

                    <ScrollView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_marginLeft="@dimen/_40sdp"
                        android:layout_marginRight="@dimen/_40sdp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginTop="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_10sdp">

                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="7"
                                    android:id="@+id/t7"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    />
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="8"
                                    android:id="@+id/t8"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginLeft="4dp"/>
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="9"
                                    android:id="@+id/t9"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginLeft="4dp"/>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_10sdp">

                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="4"
                                    android:id="@+id/t4"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginTop="4dp"/>
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="5"
                                    android:id="@+id/t5"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginLeft="4dp"
                                    android:layout_marginTop="4dp"/>
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="6"
                                    android:id="@+id/t6"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginLeft="4dp"
                                    android:layout_marginTop="4dp"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_10sdp">

                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="1"
                                    android:id="@+id/t1"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginTop="4dp"/>
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="2"
                                    android:id="@+id/t2"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginLeft="4dp"/>
                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="3"
                                    android:id="@+id/t3"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginLeft="4dp"/>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginBottom="10dp"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:layout_marginRight="@dimen/_10sdp">

                                <Button
                                    android:layout_width="0dp"
                                    android:layout_weight="2"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:text="0"
                                    android:textSize="@dimen/_18ssp"
                                    android:textStyle="bold"
                                    android:id="@+id/t0"
                                    android:layout_marginTop="4dp"
                                    android:backgroundTint="@color/md_blue_A200"/>

                                <ImageButton
                                    android:layout_width="0dp"
                                    android:layout_weight="1"
                                    android:layout_height="@dimen/_60sdp"
                                    android:layout_margin="@dimen/_5sdp"
                                    android:id="@+id/tDel"
                                    android:backgroundTint="@color/md_blue_A200"
                                    android:src="@drawable/ic_back"
                                    android:tintMode="multiply"
                                    app:tint="@color/white" />

                            </LinearLayout>

                        </LinearLayout>

                    </ScrollView>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </FrameLayout>


            <TextView
                android:id="@+id/text_home"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:textAlignment="center"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:visibility="gone"/>

            <com.facebook.shimmer.ShimmerFrameLayout
                android:layout_width="0dp"
                android:layout_weight="1.5"
                android:layout_height="match_parent"
                android:id="@+id/shimmer1"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <include layout="@layout/placeholder_home"/>
                </RelativeLayout>

            </com.facebook.shimmer.ShimmerFrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:id="@+id/frameMain"
                android:visibility="visible">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/titleMenuCuciMobil"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Menu Cuci"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            android:layout_margin="10dp"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12ssp"
                            android:fontFamily="@font/lato_bold"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleCategoryCuci"
                            app:layout_constraintTop_toBottomOf="@id/titleMenuCuciMobil"
                            android:layout_marginTop="@dimen/_3sdp"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleItem"
                            app:layout_constraintTop_toBottomOf="@id/recycleCategoryCuci"
                            android:layout_marginTop="@dimen/_2sdp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </ScrollView>



            </FrameLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorTextGray2"
                android:layout_marginLeft="@dimen/_1sdp"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                />

            <com.facebook.shimmer.ShimmerFrameLayout
                android:layout_width="0dp"
                android:layout_weight="1.5"
                android:layout_height="match_parent"
                android:id="@+id/shimmer2"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <include layout="@layout/placeholder_home"/>
                </RelativeLayout>

            </com.facebook.shimmer.ShimmerFrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:id="@+id/frameCoffee"
                android:visibility="visible">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/titleMenuCoffee"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Menu Coffee"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            android:layout_margin="10dp"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12ssp"
                            android:fontFamily="@font/lato_bold"
                            app:layout_constraintRight_toRightOf="parent"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleCategoryCoffee"
                            app:layout_constraintTop_toBottomOf="@id/titleMenuCoffee"
                            android:layout_marginTop="@dimen/_3sdp"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleItemCoffee"
                            app:layout_constraintTop_toBottomOf="@id/recycleCategoryCoffee"
                            android:layout_marginTop="@dimen/_2sdp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </ScrollView>



            </FrameLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorTextGray2"
                android:layout_marginLeft="@dimen/_1sdp"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                />



            <com.facebook.shimmer.ShimmerFrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:id="@+id/shimmer3"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <include layout="@layout/placeholder_home"/>
                </RelativeLayout>

            </com.facebook.shimmer.ShimmerFrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/framePromo"
                android:visibility="visible">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/titleMenuPromo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Menu Promo"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            android:layout_margin="10dp"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12ssp"
                            android:fontFamily="@font/lato_bold"
                            app:layout_constraintRight_toRightOf="parent"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleCategoryPromo"
                            app:layout_constraintTop_toBottomOf="@id/titleMenuPromo"
                            android:layout_marginTop="@dimen/_3sdp"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/recycleItemPromo"
                            app:layout_constraintTop_toBottomOf="@id/recycleCategoryPromo"
                            android:layout_marginTop="@dimen/_2sdp"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </ScrollView>


            </FrameLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.1"
                android:layout_marginRight="3dp"
                android:layout_marginLeft="3dp"
                android:id="@+id/relaCart"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:text="Daftar Keranjang"
                            android:layout_margin="10dp"
                            android:id="@+id/textTotalCart"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:textSize="@dimen/_11ssp"
                            android:fontFamily="@font/lato_bold"/>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnClear"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30sdp"
                            android:text="Hapus keranjang"
                            android:backgroundTint="@color/red"
                            android:fontFamily="@font/lato_bold"
                            android:textAllCaps="false"
                            android:textSize="@dimen/_8ssp"
                            android:layout_gravity="end|center_vertical"/>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearCart"
                            android:padding="5dp">

                            <TextView
                                android:id="@+id/textNameItem"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.8"
                                android:text="Nama Item"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:id="@+id/textQty"
                                android:textColor="@color/black"
                                android:textAlignment="center"
                                android:gravity="end"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold"
                                android:text="Jumlah"
                                tools:ignore="RtlCompat" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:id="@+id/textPriceItem"
                                android:textColor="@color/black"
                                android:textAlignment="center"
                                android:gravity="end"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="bold"
                                android:text="Harga Satuan"
                                tools:ignore="RtlCompat" />

                        </LinearLayout>


                    </LinearLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.5"
                        android:id="@+id/recycleCart">

                    </androidx.recyclerview.widget.RecyclerView>



                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:layout_marginBottom="@dimen/_5sdp">

                        <LinearLayout
                            android:id="@+id/linearTotal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:id="@+id/linearTop"
                                android:padding="@dimen/_2sdp">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="Total Harga"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:textSize="@dimen/_10ssp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/lato_regular"/>

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:textColor="@color/black"
                                    android:id="@+id/textSumTotal"
                                    android:layout_marginRight="5dp"
                                    android:textSize="@dimen/_12ssp"
                                    android:textStyle="bold"
                                    android:gravity="center_vertical"
                                    android:textAlignment="textEnd"
                                    tools:ignore="RtlCompat"
                                    android:fontFamily="@font/lato_regular"
                                    android:text="123"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:id="@+id/linearMidle"
                                app:layout_constraintTop_toBottomOf="@id/linearTop"
                                android:padding="@dimen/_2sdp">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="Dibayarkan"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical"
                                    android:textSize="@dimen/_10ssp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/lato_regular"/>

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:inputType="numberDecimal"
                                    android:digits="1234567890"
                                    android:id="@+id/et_paid"
                                    android:textColor="@color/black"
                                    android:layout_marginRight="5dp"
                                    android:textSize="@dimen/_11ssp"
                                    android:textStyle="bold"
                                    android:gravity="center_vertical"
                                    android:textAlignment="textEnd"
                                    tools:ignore="RtlCompat"
                                    />

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:inputType="numberDecimal"
                                    android:digits="1234567890"
                                    android:id="@+id/et_paid2"
                                    android:textColor="@color/black"
                                    android:layout_marginRight="5dp"
                                    android:textSize="24sp"
                                    android:textStyle="bold"
                                    android:gravity="center_vertical"
                                    android:textAlignment="textEnd"
                                    tools:ignore="RtlCompat"
                                    android:visibility="gone"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/_2sdp">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:text="Total Kembalian"
                                    android:gravity="center_vertical"
                                    android:textSize="@dimen/_11ssp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/lato_regular"/>

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1.5"
                                    android:textColor="@color/black"
                                    android:id="@+id/textReturn"
                                    android:layout_marginRight="5dp"
                                    android:textSize="@dimen/_11ssp"
                                    android:textStyle="bold"
                                    android:gravity="center_vertical"
                                    android:textAlignment="textEnd"
                                    tools:ignore="RtlCompat" />

                            </LinearLayout>

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/btnSave"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:backgroundTint="@color/blue"
                                android:text="Proses Pembayaran"
                                android:textSize="@dimen/_12ssp"
                                android:textAllCaps="false"
                                android:fontFamily="@font/lato_bold"
                                android:layout_margin="@dimen/_3sdp"
                                />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardKeranjang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_margin="@dimen/_8sdp"
            app:cardCornerRadius="@dimen/_5sdp"
            app:cardElevation="@dimen/_4sdp"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/_2sdp"
                android:background="#FFFFFF">

                <TextView
                    android:id="@+id/textTotalKeranjang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Keranjang"
                    android:fontFamily="@font/lato_regular"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp"/>

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/btnLihatKeranjang"
                    android:layout_width="@dimen/_60sdp"
